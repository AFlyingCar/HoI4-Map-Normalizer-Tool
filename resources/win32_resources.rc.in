
id ICON "icons/logo.ico"

1 VERSIONINFO
FILEVERSION @WIN32_RC_VERSION_NUMBER@
PRODUCTVERSION @WIN32_RC_VERSION_NUMBER@
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "CompanyName", ""
            VALUE "FileDescription", "@HMDT_TOOL_DESCRIPTION@"
            VALUE "FileVersion", "@HMDT_TOOL_VERSION@"
            VALUE "InternalName", "hoi4_mod_dev_tool"
            VALUE "OriginalFilename", "hoi4_mod_dev_tool.exe"
            VALUE "ProductName", "@HMDT_TOOL_NAME@"
            VALUE "ProductVersion", "@HMDT_TOOL_VERSION@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END
